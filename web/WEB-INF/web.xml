<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>detalle</servlet-name>
        <servlet-class>detalle</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>transferencias</servlet-name>
        <servlet-class>transferencias</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>listaclientes</servlet-name>
        <servlet-class>listaclientes</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>listatransferencias</servlet-name>
        <servlet-class>listatransferencias</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>validacuenta</servlet-name>
        <servlet-class>validacuenta</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>altaProducto</servlet-name>
        <servlet-class>altaProducto</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>detalle</servlet-name>
        <url-pattern>/detalle</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>transferencias</servlet-name>
        <url-pattern>/transferencias</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>listaclientes</servlet-name>
        <url-pattern>/listaclientes</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>listatransferencias</servlet-name>
        <url-pattern>/listatransferencias</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>validacuenta</servlet-name>
        <url-pattern>/validacuenta</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>altaProducto</servlet-name>
        <url-pattern>/altaProducto</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
